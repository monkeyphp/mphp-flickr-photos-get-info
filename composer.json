{
    "name": "monkeyphp/mphp-flickr-photos-get-info",
    "description": "Package for accessing the Flickr flickr.photos.getInfo api",
    "type": "library",
    "homepage": "https://github.com/monkeyphp/mphp-flickr-photos-get-info",
    "license": "MIT",
    "keywords": [
        "Flickr",
        "photos"
    ],
    "authors": [
        {
            "name": "David White",
            "email": "git@monkeyphp.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "git@monkeyphp.com"
    },
    "repositories":[
        {
            "type": "package",
            "package": {
                "name":"monkeyphp/mphp-flickr-base",
                "version":"1.0.0",
                "source": {
                    "url": "https://github.com/monkeyphp/mphp-flickr-base",
                    "type": "git",
                    "reference": "master"
                },
                "autoload" : {
                    "psr-0": {"MphpFlickrBase": "src/"}
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name":"apigen/apigen",
                "version": "2.8.0",
                "dist": {
                    "url": "https://github.com/downloads/apigen/apigen/ApiGen-2.8.0-standalone.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "monkeyphp/mphp-flickr-base": "1.0.0",
        "zendframework/zend-http": "2.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.14",
        "apigen/apigen": "*",
        "squizlabs/php_codesniffer": "1.4.4",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-0": {
            "MphpFlickrPhotosGetInfo": "src/"
        }
    }
}
